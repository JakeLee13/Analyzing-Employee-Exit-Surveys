   ],
   "source": [
    "# function to categorize employees according to career level\n",
    "def career_map(val):\n",
    "    if pd.isnull(val):\n",
    "        return np.nan\n",
    "    elif val < 3:\n",
    "        return 'New'\n",
    "    elif 3 <= val <6:\n",
    "        return 'Experienced'\n",
    "    elif 7 <= val < 10:\n",
    "        return 'Established'\n",
    "    else:\n",
    "        return 'Veteran'\n",
    "\n",
    "combined_updated['service_cat'] = combined_updated['institute_service'].apply(career_map)\n",
    "combined_updated['service_cat'].value_counts()"
   ]
  },
